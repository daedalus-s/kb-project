{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::kb-data-us-east-1-824116678613/*",
                "arn:aws:s3:::kb-data-us-east-1-824116678613",
                "arn:aws:s3:::kb-intermediate-us-east-1-824116678613/*",
                "arn:aws:s3:::kb-intermediate-us-east-1-824116678613"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:824116678613:log-group:/aws/lambda/*"
            ]
        }
    ]
}
